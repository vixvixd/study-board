{{>layout/header}}

<form>
    <div class="container mt-5">
        <div class="form-group">
            <label for="title">제목</label>
            <input name="title" class="form-control" id="title" placeholder="제목">
        </div>
        <div class="form-group">
            <label for="content">내용</label>
            <textarea name="content" class="form-control" id="content" placeholder="내용" rows="5"></textarea>
        </div>
        <div class="form-group">
            <label for="author">작성자</label>
            <input name="author" class="form-control" id="author" placeholder="작성자">
        </div>
        <button type="button" class="btn btn-dark" id="submit">제출</button>
        <a href="/" type="button" class="btn btn-outline-dark">취소</a>
    </div>
</form>


<script src="/js/app/board.js"></script>

{{>layout/footer}}